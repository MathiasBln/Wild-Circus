        <title>{% block title %}Welcome!{% endblock %}</title>

        {% block stylesheets %}
            <link rel="shortcut icon" type="image/png" href="/favicon.jpg">

            {{ encore_entry_link_tags('app') }}
        {% endblock %}

    </head>

    <body>

